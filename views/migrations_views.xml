<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <data>
        <record id="account_journal_migration_form" model="ir.ui.view">
            <field name="name">view.migration.id.form</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form"></field>
            <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Migracion">
                    <group>
                        <field name="old_id" readonly="1 == 0" />
                    </group>
                </page>
            </xpath>
            </field>
        </record>

    </data>
</odoo>
